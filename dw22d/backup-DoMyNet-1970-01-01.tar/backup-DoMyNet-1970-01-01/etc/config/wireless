	option vendor 'ralink'
	option channel '0'
	option autoch '2'
	option txpower '17'

config wifi-iface
	option device 'mt7620'
	option ifname 'ra0'
	option network 'lan'
	option mode 'ap'
	option ssid 'DoMyNet_9604'
	option key '123456789'
	option encryption 'psk2'

